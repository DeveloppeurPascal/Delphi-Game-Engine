{$DESCRIPTION 'Delphi Game Engine components from Patrick PREMARTIN'}
{$IMPLICITBUILD ON}

requires
  rtl;

contains
  Gamolf.RTL.Joystick in '..\..\src\Gamolf.RTL.Joystick.pas',
  Gamolf.RTL.GamepadDetected in '..\..\src\Gamolf.RTL.GamepadDetected.pas';

